{
  "name": "umw-boilerplate-static",
  "version": "1.0.0",
  "description": "Boilerplate for static angular projects",
  "main": "webpack.config.js",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist",
    "dev": "npm run clean && webpack-dev-server",
    "server": "json-server --watch db.json --port 3004",
    "test": "npm run lint",
    "lint": "eslint . && tslint -p tsconfig.json -c tslint.json"
  },
